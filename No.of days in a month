#include <stdio.h>
void main()
{
    int y,m;
    printf("enter year");
    scanf("%d",&y);
    printf("enter the month number");
    scanf("%d",&m);
    if(m==2)
    {
       if(y%4==0)
    {
        if(y%100==0)
        {
            if(y%400==0)
            {
                printf("29 days");
            }
            else
            {
                printf("28 days");
            }
        }
        else
        {
            printf("29 days");
        }
    }
    else
    {
        printf("28 days");
    }
   }
   else if(m==1||m==3||m==5||m==7||m==8 || m==10 || m==12)
   {
       printf("31 days");
   }
   else{
    printf("30 days");
   }
}
